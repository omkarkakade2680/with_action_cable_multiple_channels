<p id="notice"><%= notice %></p>
<section id="rooms" data-channel="rooms" data-room-id="<%= @room.name %>"></section>
<p>
  <strong>Name:</strong>
  <%= @room.name %>

</p>

<%= form_for @message, remote: true do |f| %>
  <%= f.text_field :content %>
  <%= f.hidden_field :room_name, :value => @room.name %>
  <%= f.submit %>
<% end %>
<div id="msg"></div>
